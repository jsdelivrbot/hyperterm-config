    "build": "babel ./src --out-dir ./dist --source-maps --copy-files",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.4.1",
    "electron": "^1.4.15",
    "fs-extra": "^1.0.0",
    "moment": "^2.15.2",
    "rimraf": "^2.6.1",
    "simple-git": "^1.66.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.1.10",
    "babel-preset-stage-0": "^6.22.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "tagets": {
            "electron": 1.4,
            "node": 6
          }
        }
